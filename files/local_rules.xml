<!-- Ansible managed! -->
<!-- Overrides built-in rules (git@github.com:wazuh/wazuh-ruleset.git) -->
<!-- https://documentation.wazuh.com/current/user-manual/ruleset/custom.html#changing-an-existing-rule -->

<group name="ossec,pam,">
  <rule id="150510" level="0" noalert="1">
    <if_sid>510</if_sid>
    <match>/dev/.blkid.tab</match>
    <match>Robust partition scheme</match>
    <match>/etc/debian_version</match>
    <description>No alert for some of Host-based anomaly detection events (rootcheck)</description>
    <group>rootcheck,</group>
  </rule>

  <rule id="516" level="0" overwrite="yes" noalert="1">
    <if_sid>510</if_sid>
    <match>^System Audit</match>
    <description>No alert for System Audit event.</description>
    <group>rootcheck,</group>
  </rule>

  <rule id="5502" level="0" overwrite="yes" noalert="1">
    <if_sid>5500</if_sid>
    <match>session closed for user </match>
    <description>No alert for PAM: Login session closed.</description>
    <group>pci_dss_10.2.5,</group>
  </rule>
</group>
